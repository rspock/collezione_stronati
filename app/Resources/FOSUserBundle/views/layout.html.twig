{% extends '::base.html.twig' %}
{% block flashMessage %}
	{% for type, messages in app.session.flashBag.all %}
            {% for message in messages %}
                <div class="{{ type }}">
                    {{ message|trans({}, 'FOSUserBundle') }}
                </div>
            {% endfor %}
        {% endfor %}
{% endblock flashMessage %}

{% block container -%}
    <div class="row">
        <div class="col-lg-12">
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </div>


{% endblock %}
