{% extends '::base.html.twig' %}

{% block container -%}
    <h1>Produttore</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ entity.nome }}</td>
            </tr>
            <tr>
                <th>Datacreazione</th>
                <td>{{ entity.dataCreazione|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datachiusura</th>
                <td>{{ entity.dataChiusura|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Datafondazione</th>
                <td>{{ entity.dataFondazione|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('produttore') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('produttore_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
